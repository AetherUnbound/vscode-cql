{
  "name": "cql",
  "version": "0.6.0",
  "displayName": "Clinical Quality Language (CQL)",
  "description": "Syntax highlighting, linting, and execution for the HL7 Clinical Quality Language (CQL) for VS Code",
  "publisher": "cqframework",
  "license": "Apache-2.0",
  "author": "Alphora",
  "bugs": "https://github.com/cqframework/vscode-cql/issues",
  "keywords": [
    "cqframework",
    "clinical quality language",
    "cql",
    "fhir",
    "hl7"
  ],
  "icon": "icons/cqframework.png",
  "preview": true,
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": false
    },
    "virtualWorkspaces": false
  },
  "engines": {
    "vscode": ">=1.63.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cqframework/vscode-cql"
  },
  "categories": [
    "Programming Languages",
    "Linters",
    "Formatters",
    "Snippets"
  ],
  "activationEvents": [
    "onLanguage:cql"
  ],
  "main": "./dist/extension",
  "contributes": {
    "languages": [
      {
        "id": "cql",
        "extensions": [
          ".cql"
        ],
        "configuration": "./language-configuration.json",
        "aliases": [
          "CQL",
          "Cql"
        ]
      }
    ],
    "grammars": [
      {
        "language": "cql",
        "scopeName": "source.cql",
        "path": "./syntaxes/cql.tmLanguage.json"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "CQL"
    },
    "commands": [
      {
        "command": "cql.open.serverLog",
        "title": "Open Server Logs",
        "category": "CQL"
      },
      {
        "command": "cql.open.clientLog",
        "title": "Open Client Logs",
        "category": "CQL"
      },
      {
        "command": "cql.open.logs",
        "title": "Open Logs",
        "category": "CQL"
      },
      {
        "command": "cql.action.viewElm",
        "title": "View ELM",
        "category": "CQL"
      },
      {
        "command": "cql.action.executeCql",
        "title": "Execute CQL",
        "category": "CQL"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "cql.action.viewElm",
          "when": "editorLangId == cql",
          "group": "1_cqlactions"
        },
        {
          "command": "cql.action.executeCql",
          "when": "editorLangId == cql",
          "group": "1_cqlactions"
        }
      ]
    }
  },
  "resolutions": {
    "color-name": "1.1.3"
  },
  "scripts": {
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=dist/extension.js --external:vscode --format=cjs --platform=node",
    "build": "npm run -S esbuild-base -- --sourcemap",
    "watch": "npm run -S esbuild-base -- --sourcemap --watch",
    "fmt": "prettier --write \"src/**/*.ts\"&& npm run test -- --fix",
    "pretest": "eslint src --ext ts && tsc --noEmit && tsc -p ./",
    "test": "node ./dist/test/suite/runTest.js"
  },
  "javaDependencies": {
    "cql-language-server": {
      "groupId": "org.opencds.cqf.cql.ls",
      "artifactId": "cql-ls-service",
      "version": "2.0.0"
    }
  },
  "devDependencies": {
    "@types/expand-tilde": "^2.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.183",
    "@types/mocha": "^9.1.0",
    "@types/node": "^16.11.7",
    "@types/node-fetch": "^2.6.2",
    "@types/vscode": "^1.34.0",
    "@types/winreg": "^1.2.31",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vscode/test-electron": "^1.6.1",
    "esbuild": "^0.15.5",
    "eslint": "^8.28.0",
    "mocha": "^9.2.1",
    "prettier": "^2.8.0",
    "source-map-support": "^0.5.21",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "expand-tilde": "^2.0.2",
    "find-java-home": "1.2.2",
    "fs-extra": "^8.1.0",
    "glob": "^7.2.0",
    "lodash": "^4.17.21",
    "node-fetch": "2.6.7",
    "tslib": "^2.4.0",
    "vscode-languageclient": "^7.0.0",
    "winreg": "^1.2.4",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "tabWidth": 2,
    "arrowParens": "avoid"
  }
}
