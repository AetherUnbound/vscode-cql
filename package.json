{
  "name": "cql",
  "version": "0.1.1",
  "displayName": "Clinical Quality Language (CQL)",
  "description": "Syntax highlighting, linting, and execution for the HL7 Clinical Quality Language (CQL) for VS Code",
  "publisher": "cqframework",
  "license": "Apache-2.0",
  "author": "Alphora",
  "bugs": "https://github.com/cqframework/vscode-cql/issues",
  "keywords": [
    "cqframework",
    "clinical quality language",
    "cql",
    "fhir",
    "hl7"
  ],
  "icon": "icons/cqframework.png",
  "preview": true,
  "enableProposedApi": false,
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": false
    },
    "virtualWorkspaces": false
  },
  "engines": {
    "vscode": "^1.53.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cqframework/vscode-cql"
  },
  "categories": [
    "Programming Languages",
    "Linters",
    "Formatters"
  ],
  "activationEvents": [
    "onLanguage:cql"
  ],
  "main": "./dist/extension",
  "contributes": {
    "languages": [
      {
        "id": "cql",
        "extensions": [
          ".cql"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "cql",
        "scopeName": "source.cql",
        "path": "./syntaxes/cql.tmLanguage.json"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "CQL configuration"
    },
    "commands": [
      {
        "command": "cql.open.serverLog",
        "title": "Open Server Logs",
        "category": "CQL"
      },
      {
        "command": "cql.open.clientLog",
        "title": "Open Client Logs",
        "category": "CQL"
      },
      {
        "command": "cql.open.logs",
        "title": "Open Logs",
        "category": "CQL"
      },
      {
        "command": "cql.action.viewElm",
        "title": "View ELM",
        "category": "CQL"
      },
      {
        "command": "cql.action.executeCql",
        "title": "Execute CQL",
        "category": "CQL"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "cql.action.viewElm",
          "when": "editorLangId == cql",
          "group": "1_cqlactions"
        },
        {
          "command": "cql.action.executeCql",
          "when": "editorLangId == cql",
          "group": "1_cqlactions"
        }
      ]
    }
  },
  "resolutions": {
    "minimist": "^1.2.5",
    "yargs-parser": ">=18.1.2",
    "glob-parent": ">=5.1.2"
  },
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "vscode:prepublish": "webpack --mode production",
    "compile": "tsc -p ./&webpack --mode development",
    "watch": "webpack --mode development --watch",
    "test": "node ./node_modules/vscode/bin/test",
    "webpack": "webpack --mode development",
    "tslint": "tslint -p ."
  },
  "javaDependencies": {
    "cql-language-server": {
      "groupId": "org.opencds.cqf.cql.ls",
      "artifactId": "cql-ls",
      "version": "1.5.2",
      "classifier": "shaded"
    }
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.0",
    "@types/glob": "^5.0.37",
    "@types/lodash.findindex": "^4.6.6",
    "@types/mocha": "^5.2.5",
    "@types/node": "^8.10.51",
    "@types/vscode": "^1.53.0",
    "@types/winreg": "^1.2.30",
    "gulp": "^4.0.2",
    "gulp-decompress": "^3.0.0",
    "gulp-download": "0.0.1",
    "lodash.findindex": "^4.6.0",
    "lodash.template": ">=4.5.0",
    "minimist": ">=1.2.5",
    "mocha": "^8.1.1",
    "ts-loader": "^9.2.6",
    "tslint": "^6.1.3",
    "typescript": "^4.4.3",
    "typescript-tslint-plugin": "^1.0.1",
    "vscode-test": "^1.6.1",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "expand-home-dir": "^0.0.3",
    "find-java-home": "1.2.2",
    "fmtr": "^1.1.2",
    "fs-extra": "^8.1.0",
    "glob": "^7.2.0",
    "node-fetch": "^3.0.0",
    "vscode-languageclient": "^7.0.0",
    "winreg-utf8": "^0.1.1",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.10.0"
  }
}
