{
  "name": "cql",
  "version": "0.1.0",
  "displayName": "Clinical Quality Language (CQL) for VS Code",
  "description": "Syntax highlighting, linting, and execution for the HL7 Clinical Quality Language (CQL)",
  "publisher": "cqframework",
  "license": "Apache-2.0",
  "author": "Alphora",
  "bugs": "https://github.com/DBCG/vscode-cql/issues",
  "keywords": [
    "cqframework",
    "clinical quality language",
    "cql",
    "fhir",
    "hl7"
  ],
  "icon": "icons/cqframework.png",
  "preview": true,
  "enableProposedApi": false,
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": false
    },
    "virtualWorkspaces": false
  },
  "engines": {
    "vscode": "^1.53.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DBCG/vscode-cql"
  },
  "categories": [
    "Programming Languages",
    "Linters",
    "Formatters"
  ],
  "activationEvents": [
    "onLanguage:cql"
  ],
  "main": "./dist/extension",
  "contributes": {
    "languages": [
      {
        "id": "cql",
        "extensions": [
          ".cql"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "cql",
        "scopeName": "source.cql",
        "path": "./syntaxes/cql.tmLanguage.json"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "CQL configuration"
    },
    "commands": [
      {
        "command": "cql.open.serverLog",
        "title": "Open Server Logs",
        "category": "CQL"
      },
      {
        "command": "cql.open.clientLog",
        "title": "Open Client Logs",
        "category": "CQL"
      },
      {
        "command": "cql.open.logs",
        "title": "Open Logs",
        "category": "CQL"
      }
    ]
  },
  "resolutions": {
    "minimist": "^1.2.5",
    "yargs-parser": "^18.1.2"
  },
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "vscode:prepublish": "webpack --mode production",
    "compile": "tsc -p ./&webpack --mode development",
    "watch": "webpack --mode development --watch --info-verbosity verbose",
    "test": "node ./node_modules/vscode/bin/test",
    "webpack": "webpack --mode development",
    "tslint": "tslint -p ."
  },
  "javaDependencies": {
    "cql_language_server": "1.5.2"
  },
  "extensionDependencies": [
    "mindaro-dev.file-downloader"
  ],
  "devDependencies": {
    "@types/fs-extra": "^8.0.0",
    "@types/glob": "5.0.30",
    "@types/lodash.findindex": "^4.6.6",
    "@types/mocha": "^5.2.5",
    "@types/node": "^8.10.51",
    "@types/vscode": "^1.53.0",
    "@types/winreg": "^1.2.30",
    "gulp": "^4.0.2",
    "gulp-decompress": "^3.0.0",
    "gulp-download": "0.0.1",
    "lodash.findindex": "^4.6.0",
    "lodash.template": ">=4.5.0",
    "minimist": ">=1.2.5",
    "mocha": "^8.1.1",
    "ts-loader": "^5.3.1",
    "tslint": "^5.11.0",
    "typescript": "^4.2.4",
    "typescript-tslint-plugin": "^0.3.1",
    "vscode-test": "^1.4.0",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "expand-home-dir": "^0.0.3",
    "find-java-home": "1.1.0",
    "fmtr": "^1.1.2",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.3",
    "nodejs-file-downloader": "^4.7.4",
    "vscode-languageclient": "^7.0.0",
    "vscode-read-manifest": "^0.5.1",
    "winreg-utf8": "^0.1.1",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.10.0"
  }
}
